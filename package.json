{
  "name": "contentful-sdk-core",
  "version": "0.0.1-determined-by-semantic-release",
  "description": "Core modules for the Contentful JS SDKs",
  "homepage": "https://www.contentful.com/developers/docs/javascript/",
  "main": "dist/index.js",
  "module": "dist/index.es-modules.js",
  "jsnext:main": "dist/index.es-modules.js",
  "browser": {
    "os": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/contentful-sdk-core.git"
  },
  "author": "Contentful <opensource@contentful.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf coverage && rimraf dist",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "lint": "eslint lib test --ext '.ts,.js'",
    "pretest": "npm run lint",
    "test": "npm run test:cover",
    "test:ci-emulate": "trevor",
    "test:cover": "babel-node --extensions .ts --extensions .js ./node_modules/istanbul/lib/cli.js cover ./test/runner | tap-spec",
    "test:unit": "babel-node --extensions .ts --extensions .js ./test/runner | tap-spec",
    "test:debug": "babel-node --extensions .ts --extensions .js debug ./test/runner",
    "browser-coverage": "npm run test:cover && opener coverage/lcov-report/index.html",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "semantic-release": "semantic-release",
    "precommit": "npm run lint",
    "prepush": "npm run test:unit"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "fast-copy": "^2.1.0",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/register": "^7.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "axios": "^0.21.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-plugin-rewire": "^1.2.0",
    "blue-tape": "^1.0.0",
    "codecov": "^3.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.2.5",
    "in-publish": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mkdirp": "^1.0.4",
    "moxios": "^0.4.0",
    "opener": "^1.4.1",
    "require-all": "^3.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.0",
    "rollup-plugin-babel": "^4.4.0",
    "semantic-release": "^17.0.7",
    "sinon": "^9.0.2",
    "tap-spec": "^5.0.0",
    "trevor": "^2.3.0",
    "tslib": "^2.0.3",
    "typescript": "4.1.2"
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          {
            "type": "build",
            "scope": "deps",
            "release": "patch"
          }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/changelog",
    "@semantic-release/github"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
